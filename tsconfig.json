{
  "compilerOptions": {
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es7",
      "esnext.asynciterable"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "target": "ES6",
    "rootDir": "./",
    "outDir": ".build"
  }
}
